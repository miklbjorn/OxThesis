@article{dennerElectroweakRadiativeCorrections2000,
  title = {Electroweak Radiative Corrections to E+e- --{$>$} {{WW}} --{$>$} 4fermions in Double-Pole Approximation -- the {{RACOONWW}} Approach},
  author = {Denner, A. and Dittmaier, S. and Roth, M. and Wackeroth, D.},
