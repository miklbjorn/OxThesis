  eprint = {1004.5053},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/5QU66F9D/The BABAR Collaboration and Sanchez - 2010 - Measurement of D0-antiD0 mixing parameters using D.pdf;/Users/bjoern/Zotero/storage/TDQBQSIW/1004.html},
