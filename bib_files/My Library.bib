  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/BZWFEHH4/The ALEPH Collaboration - 2006 - Measurement of the W boson mass and width in e+e- .pdf;/Users/bjoern/Zotero/storage/3X9NM5A2/0605011.html},
  journal = {The European Physical Journal C},
  keywords = {High Energy Physics - Experiment},
  number = {2}
}

