  keywords = {derivative: covariance,effective field theory,expansion: derivative,expansion: higher-order,Higgs particle: coupling,higher-order: 1,minimal supersymmetric standard model,new physics,operator: dimension: 6,Phenomenology of Field Theories in Higher Dimensions,stop: mass,supersymmetry,Supersymmetry Phenomenology,unitarity}
