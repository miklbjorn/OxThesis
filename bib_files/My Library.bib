@article{benekeFourfermionProductionPair2008,
  title = {Four-Fermion Production near the {{W}} Pair Production Threshold},
  author = {Beneke, Martin and Falgari, Pietro and Schwinn, Christian and Signer, Adrian and Zanderighi, Giulia},
  year = {2008},
  month = mar,
  volume = {792},
  pages = {89--135},
  issn = {05503213},
  doi = {10.1016/j.nuclphysb.2007.09.030},
  abstract = {We perform a dedicated study of the four-fermion production process e- e+ -{$>$} mu- nubar\_mu u dbar X near the W pair-production threshold in view of the importance of this process for a precise measurement of the W boson mass. Accurate theoretical predictions for this process require a systematic treatment of finite-width effects. We use unstable-particle effective field theory (EFT) to perform an expansion in the coupling constants, GammaW/MW, and the non-relativistic velocity v of the W boson up to next-to-leading order in GammaW/MW \textasciitilde{} alpha\_ew \textasciitilde{} v\^2. We find that the dominant theoretical uncertainty in MW is currently due to an incomplete treatment of initial-state radiation. The remaining uncertainty of the NLO EFT calculation translates into delta MW \textasciitilde{} 10-15 MeV, and to about 5 MeV with additional input from the NLO four-fermion calculation in the full theory.},
  archivePrefix = {arXiv},
  eprint = {0707.0773},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/6PHUKIFP/Beneke et al. - 2008 - Four-fermion production near the W pair production.pdf;/Users/bjoern/Zotero/storage/7BPMJAX4/0707.html},
  journal = {Nuclear Physics B},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology},
  number = {1-2}
}

