  doi = {10.1016/0550-3213(86)90262-2},
  abstract = {New interactions with a scale {$\Lambda$} larger than the Fermi scale GF-12 will manifest themselves at energies below {$\Lambda$} through small deviations from the standard model, which can be described by an effective lagrangian containing non-renormalizable SU(3) \texttimes{} SU(2) \texttimes{} U(1) invariant operators. We construct the first two terms of this lagrangian in an expansion in powers of 1/{$\Lambda$} and study systematically possible effects of new interactions such as anomalous magnetic moments, deviations from universality in weak interactions and rare processes. Among the flavour conserving processes the universality of the charged current weak interactions yields the strongest bound on the new interaction scale, {$\Lambda$} \&gt; 5 TeV, whereas flavour non-conserving processes imply the bound {$\Lambda$} \&gt; 3000 TeV. We derive conditions for natural flavour conservation. Although it cannot be excluded that all flavour changing operators are dynamically suppressed, this appears difficult to understand without a symmetry for which the standard electroweak theory seems to provide no hint. We emphasize the importance of searching for rare decays of D-mesons.},
  file = {/Users/bjoern/Zotero/storage/PQXZ4VPI/Buchmller and Wyler - 1986 - Effective lagrangian analysis of new interactions .pdf;/Users/bjoern/Zotero/storage/47PNPS4M/0550321386902622.html},
  journal = {Nuclear Physics B},
  number = {3\textendash 4}
}

