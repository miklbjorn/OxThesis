  file = {/Users/bjoern/Zotero/storage/YNPLDDG2/Bigi and Yamamoto - 1995 - Interference between Cabibbo allowed and doubly fo.pdf;/Users/bjoern/Zotero/storage/53B8B48B/037026939500285S.html},
  journal = {Physics Letters B},
  language = {en},
  number = {3}
}

