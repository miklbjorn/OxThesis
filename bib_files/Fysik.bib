  doi = {10.1103/PhysRevLett.105.121801},
  abstract = {We report the measurement of the Cabibbo-Kobayashi-Maskawa CP-violating angle \$\textbackslash gamma\$ through a Dalitz plot analysis of neutral \$D\$ meson decays to \$K\_S\^0 \textbackslash pi\^+ \textbackslash pi\^-\$ and \$K\_S\^0 K\^+ K\^-\$ produced in the processes \$B\^\textbackslash mp \textbackslash to D K\^\textbackslash mp\$, \$B\^\textbackslash mp \textbackslash to D\^\{*\} K\^\textbackslash mp\$ with \$D\^* \textbackslash to D \textbackslash pi\^0,D \textbackslash gamma\$, and \$B\^\textbackslash mp \textbackslash to D K\^\{*\textbackslash mp\}\$ with \$K\^\{*\textbackslash mp\} \textbackslash to K\_S\^0 \textbackslash pi\^\textbackslash mp\$, using 468 million \$B\textbackslash bar\{B\}\$ pairs collected by the BABAR detector at the PEP-II asymmetric-energy \$e\^+e\^-\$ collider at SLAC. We measure \$\textbackslash gamma=(68 \textbackslash pm 14 \textbackslash pm 4 \textbackslash pm 3)\^\textbackslash circ\$ (\{\textbackslash rm mod \$180\^\textbackslash circ\$\}), where the first error is statistical, the second is the experimental systematic uncertainty and the third reflects the uncertainty in the description of the neutral \$D\$ decay amplitudes. This result is inconsistent with \$\textbackslash gamma = 0\$ (no direct CP violation) with a significance of 3.5 standard deviations.},
  archivePrefix = {arXiv},
  eprint = {1005.1096},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/KIHID8AA/The BABAR Collaboration and Sanchez - 2010 - Evidence for direct CP violation in the measuremen.pdf;/Users/bjoern/Zotero/storage/AAMPZ2PT/1005.html},
