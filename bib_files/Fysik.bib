@article{thebellecollaborationEvidenceDirectCP2010,
  title = {Evidence for Direct {{CP}} Violation in the Decay {{B}}-{$>$}{{D}}(*){{K}}, {{D}}-{$>$}{{KsPi}}+{{Pi}}- and Measurement of the {{CKM}} Phase Phi3},
  author = {The Belle Collaboration and Poluektov, A. and Bondar, A. and Yabsley, B. D.},
  year = {2010},
  month = jun,
  volume = {81},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.81.112002},
  abstract = {We present a new measurement of the unitarity triangle angle phi3 using a Dalitz plot analysis of the KsPi+Pi- decay of the neutral D meson produced in B-{$>$}D(*)K decays. The method exploits the interference between D0 and D0bar to extract the angle phi3, strong phase delta and the ratio r of suppressed and allowed amplitudes. We apply this method to a 605 fb-1 data sample collected by the Belle experiment. The analysis uses three decays: B-{$>$}DK, and B-{$>$}D*K with D*-{$>$}DPi0 and D*-{$>$}Dgamma, as well as the corresponding charge-conjugate modes. From a combined maximum likelihood fit to the three modes, we obtain phi3 = 78.4\^+10.8\_-11.6 +- 3.6 (syst) +- 8.9 (model) degrees. CP conservation in this process is ruled out at the confidence level (1-CL)=5x10\^-4, or 3.5 standard deviations.},
  archivePrefix = {arXiv},
  eprint = {1003.3360},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/PYQKJW9K/The Belle Collaboration et al. - 2010 - Evidence for direct CP violation in the decay B-D.pdf;/Users/bjoern/Zotero/storage/FBW2FV8T/1003.html},
  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {11}
}

@article{thecmscollaborationObservationNewBoson2012,
  title = {Observation of a New Boson at a Mass of 125 {{GeV}} with the {{CMS}} Experiment at the {{LHC}}},
  author = {The CMS Collaboration},
  year = {2012},
  month = sep,
  volume = {716},
  pages = {30--61},
  issn = {03702693},
  doi = {10.1016/j.physletb.2012.08.021},
  abstract = {Results are presented from searches for the standard model Higgs boson in proton-proton collisions at sqrt(s) = 7 and 8 TeV in the Compact Muon Solenoid experiment at the LHC, using data samples corresponding to integrated luminosities of up to 5.1 inverse femtobarns at 7 TeV and 5.3 inverse femtobarns at 8 TeV. The search is performed in five decay modes: gamma gamma, ZZ, WW, tau tau, and b b-bar. An excess of events is observed above the expected background, with a local significance of 5.0 standard deviations, at a mass near 125 GeV, signalling the production of a new particle. The expected significance for a standard model Higgs boson of that mass is 5.8 standard deviations. The excess is most significant in the two decay modes with the best mass resolution, gamma gamma and ZZ; a fit to these signals gives a mass of 125.3 +/- 0.4 (stat.) +/- 0.5 (syst.) GeV. The decay to two photons indicates that the new particle is a boson with spin different from one.},
  archivePrefix = {arXiv},
  eprint = {1207.7235},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/4NSKQKN7/The CMS Collaboration - 2012 - Observation of a new boson at a mass of 125 GeV wi.pdf;/Users/bjoern/Zotero/storage/464DFZWC/1207.html},
  journal = {Physics Letters B},
  keywords = {High Energy Physics - Experiment},
  number = {1}
}

@article{thedelphicollaborationMeasurementMassWidth2008,
  title = {Measurement of the {{Mass}} and {{Width}} of the {{W Boson}} in E+e- {{Collisions}} at Sqrt\{s\} = 161 - 209 {{GeV}}},
  author = {The DELPHI Collaboration and Abdallah, J.},
  year = {2008},
  month = may,
  volume = {55},
  pages = {1--38},
  issn = {1434-6044, 1434-6052},
  doi = {10.1140/epjc/s10052-008-0585-7},
  abstract = {A measurement of the W boson mass and width has been performed by the DELPHI collaboration using the data collected during the full LEP2 programme (1996-2000). The data sample has an integrated luminosity of 660 pb\^-1 and was collected over a range of centre-of-mass energies from 161 to 209 GeV. Results are obtained by applying the method of direct reconstruction of the mass of the W from its decay products in both the W+W- -{$>$} lvqq and W+W- -{$>$} qqqq channels. The W mass result for the combined data set is M\_W = 80.336 +/- 0.055 (Stat.) +/- 0.028 (Syst.) +/- 0.025 (FSI) +/- 0.009 (LEP) GeV/c\^2, where FSI represents the uncertainty due to final state interaction effects in the qqqq channel, and LEP represents that arising from the knowledge of the collision energy of the accelerator. The combined value for the W width is Gamma\_W = 2.404 +/- 0.140 (Stat.) +/- 0.077 (Syst.) +/- 0.065 (FSI) GeV/c\^2. These results supersede all values previously published by the DELPHI collaboration. This paper is dedicated to the memory of Carlo Caso.},
  archivePrefix = {arXiv},
  eprint = {0803.2534},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/AZWXJJDN/The DELPHI Collaboration and Abdallah - 2008 - Measurement of the Mass and Width of the W Boson i.pdf;/Users/bjoern/Zotero/storage/2HFBMX29/0803.html},
  journal = {The European Physical Journal C},
  keywords = {High Energy Physics - Experiment},
  number = {1}
}

@article{thedelphicollaborationMeasurementsCPconservingTrilinear2010,
  title = {Measurements of {{CP}}-Conserving {{Trilinear Gauge Boson Couplings WWV}} ({{V}} = Gamma,{{Z}}) in E+e- {{Collisions}} at {{LEP2}}},
  author = {The DELPHI Collaboration},
