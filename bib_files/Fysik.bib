  file = {/Users/bjoern/Zotero/storage/7RGCBMD3/Tevatron Electroweak Working Group m.fl. - 2012 - 2012 Update of the Combination of CDF and D0 Resul.pdf;/Users/bjoern/Zotero/storage/7N2JZJX7/1204.html},
