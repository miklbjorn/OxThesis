@article{cdfcollaborationDirectMeasurementBoson2008,
  title = {A Direct {{Measurement}} of the {{W Boson Width}} in Ppbar {{Collisions}} at Sqrt(s) = 1.96 {{TeV}}},
  author = {CDF Collaboration and Aaltonen, T.},
  year = {2008},
  month = feb,
  volume = {100},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.100.071801},
  abstract = {A direct measurement of the total decay width of the W boson G\_W is presented using 350 pb-1 of data from ppbar collisions at sqrt(s) = 1.96 TeV collected with the CDF II detector at the Fermilab Tevatron. The width is determined by normalizing predicted signal and background distributions to 230185 W candidates decaying to enu and munu in the transverse-mass region 50 {$<$} M\_T {$<$} 90 GeV and then fitting the predicted shape to 6055 events in the high-M\_T region, 90 {$<$} M\_T {$<$} 200 GeV. The result is G\_W = 2032 +/- 45 (stat.) +/- 57 (syst.) MeV, consistent with the standard model expectation.},
  archivePrefix = {arXiv},
  eprint = {0710.4112},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/PVGRK4W4/CDF Collaboration and Aaltonen - 2008 - A direct Measurement of the W Boson Width in ppbar.pdf;/Users/bjoern/Zotero/storage/E4QW5HZC/0710.html},
  journal = {Physical Review Letters},
  keywords = {High Energy Physics - Experiment},
  number = {7}
}

@article{cdfcollaborationMeasurementsBranchingFraction2010,
  title = {Measurements of Branching Fraction Ratios and {{CP}} Asymmetries in {{B}}+/- -{$>$}{{D}}\_{{CP K}}+/- Decays in Hadron Collisions},
  author = {CDF Collaboration},
  year = {2010},
  month = feb,
  volume = {81},
  pages = {031105},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.81.031105},
  abstract = {We reconstruct B+/- -{$>$} D K+/- decays in a data sample collected by the CDF II detector at the Tevatron collider corresponding to 1 fb-1 of integrated luminosity. We select decay modes where the D meson decays to either K- pi+ (flavor eigenstate) or K- K+, pi- pi+ (CP-even eigenstates), and measure the direct CP asymmetry A\_CP+ = 0.39 +/- 0.17(stat) +/- 0.04(syst), and the double ratio of CP-even to flavor eigenstate branching fractions R\_CP+ = 1.30 +/- 0.24(stat) +/- 0.12(syst). These measurements will improve the determination of the CKM angle gamma. They are performed here for the first time using data from hadron collisions.},
  archivePrefix = {arXiv},
  eprint = {0911.0425},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/3RU7ABJE/CDF Collaboration - 2010 - Measurements of branching fraction ratios and CP a.pdf;/Users/bjoern/Zotero/storage/S5EMLM25/0911.html},
  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {3}
}

@article{cdfcollaborationMeasurementsBranchingFraction2011,
  title = {Measurements of Branching Fraction Ratios and {{CP}}-Asymmetries in Suppressed {{B}}\^- -{$>$} {{D}}(-{$>$} {{K}}\^+ Pi\^-){{K}}\^- and {{B}}\^- -{$>$} {{D}}(-{$>$} {{K}}\^+ Pi\^-)Pi\^- Decays},
  author = {CDF Collaboration},
  year = {2011},
  month = nov,
  volume = {84},
  pages = {091504},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.84.091504},
  abstract = {We report the first reconstruction in hadron collisions of the suppressed decays B\^- -{$>$} D(-{$>$} K\^+ pi\^-)K\^- and B\^- -{$>$} D(-{$>$} K\^+ pi\^-)pi\^-, sensitive to the CKM phase gamma, using data from 7 fb\^-1 of integrated luminosity collected by the CDF II detector at the Tevatron collider. We reconstruct a signal for the B\^- -{$>$} D(-{$>$} K\^+ pi\^-)K\^- suppressed mode with a significance of 3.2 standard deviations, and measure the ratios of the suppressed to favored branching fractions R(K) = [22.0 \textbackslash pm 8.6(stat)\textbackslash pm 2.6(syst)]\textbackslash times 10\^-3, R\^+(K) = [42.6\textbackslash pm 13.7(stat)\textbackslash pm 2.8(syst)]\textbackslash times 10\^-3, R\^-(K)= [3.8\textbackslash pm 10.3(stat)\textbackslash pm 2.7(syst]\textbackslash times 10\^-3, as well as the direct CP-violating asymmetry A(K) = -0.82\textbackslash pm 0.44(stat)\textbackslash pm 0.09(syst) of this mode. Corresponding quantities for B\^- -{$>$} D(-{$>$} K\^+ pi\^-)pi\^- decay are also reported.},
  archivePrefix = {arXiv},
  eprint = {1108.5765},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/RARRW7QW/CDF Collaboration - 2011 - Measurements of branching fraction ratios and CP-a.pdf;/Users/bjoern/Zotero/storage/3SWXAGAI/1108.html},
  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {9}
}

