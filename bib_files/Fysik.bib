@article{aeppliGammaCorrectionsPair1993,
  title = {\${{O}}(\textbackslash{{Gamma}})\$ {{Corrections}} to \${{W}}\$ Pair Production in \$e+e-\$ and \$\textbackslash gamma\textbackslash gamma\$ Collisions},
