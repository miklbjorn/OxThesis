  issn = {03702693},
  doi = {10.1016/0370-2693(93)91259-P},
  abstract = {Several schemes to introduce finite width effects to reactions involving unstable elementary particles are given and the differences between them are investigated. The effects of the different schemes is investigated numerically for \$W\$ pair production. In \$e\^+e\^-\textbackslash to W\^+W\^-\$ we find that the effect of the non-resonant graphs cannot be neglected for \$\textbackslash sqrt\{s\}\textbackslash geq400\textbackslash GeV\$. There is no difference between the various schemes to add these to the resonant graphs away from threshold, although some violate gauge invariance. On the other hand, in the reaction \$\textbackslash gamma\textbackslash gamma\textbackslash to W\^+W\^-\$ the effect of the non-resonant graphs is large everywhere, due to the \$t\$-channel pole. However, even requiring that the outgoing lepton is observable (\$p\_\textbackslash perp {$>$} .02\textbackslash sqrt\{s\}\$) reduces the contribution to about 1\textbackslash\%. Again, the scheme dependence is negligible here.},
  archivePrefix = {arXiv},
  eprint = {hep-ph/9303236},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/UWWX7FMM/Aeppli et al. - 1993 - $order(Gamma)$ Corrections to $W$ pair productio.pdf;/Users/bjoern/Zotero/storage/WZ2IZKWT/9303236.html},
  journal = {Physics Letters B},
  keywords = {High Energy Physics - Phenomenology},
  number = {3-4}
}

