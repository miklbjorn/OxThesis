@article{alephcollaborationElectroweakMeasurementsElectronPositron2013,
  title = {Electroweak {{Measurements}} in {{Electron}}-{{Positron Collisions}} at {{W}}-{{Boson}}-{{Pair Energies}} at {{LEP}}},
  author = {ALEPH Collaboration and DELPHI Collaboration and The L3 Collaboration and The OPAL Collaboration and The LEP Electroweak Working Group},
  year = {2013},
  month = nov,
  volume = {532},
  pages = {119--244},
  issn = {03701573},
  doi = {10.1016/j.physrep.2013.07.004},
  abstract = {Electroweak measurements performed with data taken at the electron-positron collider LEP at CERN from 1995 to 2000 are reported. The combined data set considered in this report corresponds to a total luminosity of about 3 fb\$\^\{-1\}\$ collected by the four LEP experiments ALEPH, DELPHI, L3 and OPAL, at centre-of-mass energies ranging from 130 GeV to 209 GeV. Combining the published results of the four LEP experiments, the measurements include total and differential cross-sections in photon-pair, fermion-pair and four-fermion production, the latter resulting from both double-resonant WW and ZZ production as well as singly resonant production. Total and differential cross-sections are measured precisely, providing a stringent test of the Standard Model at centre-of-mass energies never explored before in electron-positron collisions. Final-state interaction effects in four-fermion production, such as those arising from colour reconnection and Bose-Einstein correlations between the two W decay systems arising in WW production, are searched for and upper limits on the strength of possible effects are obtained. The data are used to determine fundamental properties of the W boson and the electroweak theory. Among others, the mass and width of the W boson, \$M\_W\$ and \$\textbackslash Gamma\_W\$, the branching fraction of W decays to hadrons, \$B(W\textbackslash to\textbackslash mathrm\{had\})\$, and the trilinear gauge-boson self-couplings \$g\^Z\_1\$, \$\textbackslash kappa\_\textbackslash gamma\$ and \$\textbackslash lambda\_\textbackslash gamma\$ are determined to be: M\_W = 80.376 \textbackslash pm 0.033 \textasciitilde GeV \textbackslash Gamma\_W = 2.195 \textbackslash pm 0.083 \textasciitilde GeV B(W\textbackslash to\textbackslash mathrm\{had\}) = 67.41 \textbackslash pm 0.27 \% g\^Z\_1 = 0.984 \^\{+0.018\}\_\{-0.020\} \textbackslash kappa\_\textbackslash gamma = 0.982 \textbackslash pm 0.042 \textbackslash lambda\_\textbackslash gamma = -0.022 \textbackslash pm 0.019},
  archivePrefix = {arXiv},
  eprint = {1302.3415},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/2QMTNW9E/The ALEPH Collaboration m.fl. - 2013 - Electroweak Measurements in Electron-Positron Coll.pdf;/Users/bjoern/Zotero/storage/RCCF5F2C/1302.html},
  journal = {Physics Reports},
  keywords = {High Energy Physics - Experiment,High Energy Physics - Phenomenology},
  number = {4}
}

@article{alonsoRenormalizationGroupEvolution2014,
  title = {Renormalization {{Group Evolution}} of the {{Standard Model Dimension Six Operators III}}: {{Gauge Coupling Dependence}} and {{Phenomenology}}},
  shorttitle = {Renormalization {{Group Evolution}} of the {{Standard Model Dimension Six Operators III}}},
  author = {Alonso, Rodrigo and Jenkins, Elizabeth E. and Manohar, Aneesh V. and Trott, Michael},
  year = {2014},
  month = apr,
  volume = {2014},
  issn = {1029-8479},
  doi = {10.1007/JHEP04(2014)159},
  abstract = {We calculate the gauge terms of the one-loop anomalous dimension matrix for the dimension-six operators of the Standard Model effective field theory (SM EFT). Combining these results with our previous results for the \$\textbackslash lambda\$ and Yukawa coupling terms completes the calculation of the one-loop anomalous dimension matrix for the dimension-six operators. There are 1350 \$CP\$-even and \$1149\$ \$CP\$-odd parameters in the dimension-six Lagrangian for 3 generations, and our results give the entire \$2499 \textbackslash times 2499\$ anomalous dimension matrix. We discuss how the renormalization of the dimension-six operators, and the additional renormalization of the dimension \$d \textbackslash le 4\$ terms of the SM Lagrangian due to dimension-six operators, lays the groundwork for future precision studies of the SM EFT aimed at constraining the effects of new physics through precision measurements at the electroweak scale. As some sample applications, we discuss some aspects of the full RGE improved result for essential processes such as \$gg \textbackslash to h\$, \$h \textbackslash to \textbackslash gamma \textbackslash gamma\$ and \$h \textbackslash to Z \textbackslash gamma\$, for Higgs couplings to fermions, for the precision electroweak parameters \$S\$ and \$T\$, and for the operators that modify important processes in precision electroweak phenomenology, such as the three-body Higgs boson decay \$h \textbackslash rightarrow Z \textbackslash, \textbackslash ell\^+ \textbackslash, \textbackslash ell\^-\$ and triple gauge boson couplings. We discuss how the renormalization group improved results can be used to study the flavor problem in the SM EFT, and to test the minimal flavor violation (MFV) hypothesis. We briefly discuss the renormalization effects on the dipole coefficient \$C\_\{e\textbackslash gamma\}\$ which contributes to \$\textbackslash mu \textbackslash to e \textbackslash gamma\$ and to the muon and electron magnetic and electric dipole moments.},
  archivePrefix = {arXiv},
  eprint = {1312.2014},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/84QFBQ84/Alonso et al. - 2014 - Renormalization Group Evolution of the Standard Mo.pdf;/Users/bjoern/Zotero/storage/MB6ACXZI/1312.html},
  journal = {Journal of High Energy Physics},
  keywords = {High Energy Physics - Phenomenology},
  number = {4}
}

