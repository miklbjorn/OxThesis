  language = {en},
  number = {5}
}

@article{ciuchiniElectroweakPrecisionObservables2013,
  title = {Electroweak {{Precision Observables}}, {{New Physics}} and the {{Nature}} of a 126 {{GeV Higgs Boson}}},
  author = {Ciuchini, Marco and Franco, Enrico and Mishima, Satoshi and Silvestrini, Luca},
  year = {2013},
  month = aug,
  volume = {2013},
  issn = {1029-8479},
  doi = {10.1007/JHEP08(2013)106},
  abstract = {We perform the fit of electroweak precision observables within the Standard Model with a 126 GeV Higgs boson, compare the results with the theoretical predictions and discuss the impact of recent experimental and theoretical improvements. We introduce New Physics contributions in a model-independent way and fit for the S, T and U parameters, for the \$\textbackslash epsilon\_\{1,2,3,b\}\$ ones, for modified \$Zb\textbackslash bar\{b\}\$ couplings and for a modified Higgs coupling to vector bosons. We point out that composite Higgs models are very strongly constrained. Finally, we compute the bounds on dimension-six operators relevant for the electroweak fit.},
  archivePrefix = {arXiv},
