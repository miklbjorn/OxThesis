  keywords = {High Energy Physics - Experiment},
  number = {12}
}

@article{thebabarcollaborationMeasurementCPObservables2010,
  title = {Measurement of {{CP}} Observables in {{B}}+- --{$>$} {{D}}\_{{CP K}}+- Decays and Constraints on the {{CKM}} Angle Gamma},
  author = {The BABAR Collaboration and Sanchez, P. del Amo},
