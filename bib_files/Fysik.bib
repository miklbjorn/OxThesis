  eprint = {0708.1311},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/PCP9QDMC/The OPAL collaboration - 2007 - Measurement of the e+e- - W+W- cross section and .pdf;/Users/bjoern/Zotero/storage/NB5DIZJV/0708.html},
  journal = {The European Physical Journal C},
  keywords = {High Energy Physics - Experiment},
  number = {4}
}

