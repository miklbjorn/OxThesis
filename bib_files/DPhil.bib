@article{thebellecollaborationEvidenceDirectCP2010,
  title = {Evidence for Direct {{CP}} Violation in the Decay {{B}}-{$>$}{{D}}(*){{K}}, {{D}}-{$>$}{{KsPi}}+{{Pi}}- and Measurement of the {{CKM}} Phase Phi3},
  author = {The Belle Collaboration and Poluektov, A. and Bondar, A. and Yabsley, B. D.},
  year = {2010},
  month = jun,
  volume = {81},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.81.112002},
  abstract = {We present a new measurement of the unitarity triangle angle phi3 using a Dalitz plot analysis of the KsPi+Pi- decay of the neutral D meson produced in B-{$>$}D(*)K decays. The method exploits the interference between D0 and D0bar to extract the angle phi3, strong phase delta and the ratio r of suppressed and allowed amplitudes. We apply this method to a 605 fb-1 data sample collected by the Belle experiment. The analysis uses three decays: B-{$>$}DK, and B-{$>$}D*K with D*-{$>$}DPi0 and D*-{$>$}Dgamma, as well as the corresponding charge-conjugate modes. From a combined maximum likelihood fit to the three modes, we obtain phi3 = 78.4\^+10.8\_-11.6 +- 3.6 (syst) +- 8.9 (model) degrees. CP conservation in this process is ruled out at the confidence level (1-CL)=5x10\^-4, or 3.5 standard deviations.},
  archivePrefix = {arXiv},
  eprint = {1003.3360},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/PYQKJW9K/The Belle Collaboration et al. - 2010 - Evidence for direct CP violation in the decay B-D.pdf;/Users/bjoern/Zotero/storage/FBW2FV8T/1003.html},
  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {11}
}

@article{thomasModelindependentOverlineDMixing2012,
  title = {{Model-independent $ {D^0}-{{\overline{D}}^0} $mixing and CP violation studies with $ {D^0}\to K_{\mathrm{S}}^0{\pi^{+}}{\pi^{-}} $and $ {D^0}\to K_{\mathrm{S}}^0{K^{+}}{K^{-}} $}},
  author = {Thomas, C. and Wilkinson, G.},
  year = {2012},
  month = oct,
  volume = {2012},
  pages = {185},
  issn = {1029-8479},
  doi = {10.1007/JHEP10(2012)185},
  abstract = {Simulation studies are performed to assess the sensitivity of a model-independent analysis of the flavour-tagged decays $ {D^0}\to K_{\mathrm{S}}^0{\pi^{+}}{\pi^{-}} $and $ {D^0}\to K_{\mathrm{S}}^0{K^{+}}{K^{-}} $to mixing and CP violation. The analysis takes as input measurements of the D decay strong-phase parameters that are accessible in quantum-correlated $ D-\overline{D} $pairs produced at the ψ(3770) resonance. It is shown that the model-independent approach is well suited to the very large data sets expected at an upgraded LHCb experiment, or future high luminosity e+e− facility, and that with 100M $ K_{\mathrm{S}}^0{\pi^{+}}{\pi^{-}} $decays a statistical precision of around 0.01 and 0.7° is achievable on the CP violation parameters rCPand αCP, respectively. Even with this very large sample the systematic uncertainties associated with the strong-phase parameters will not be limiting, assuming that full use is made of the available ψ(3770) data sets of CLEO-c and BES-III. Furthermore, it is demonstrated that large flavour-tagged samples can themselves be exploited to provide information on the strong-phase parameters, a feature that will be beneficial in the measurement of the CKM angle γ/ϕ3 with B− → DK− decays.},
  file = {/Users/bjoern/Zotero/storage/FCFFP2V8/Thomas and Wilkinson - 2012 - Model-independent $ D^0 - overline D ^0 $mixi.pdf},
  journal = {Journal of High Energy Physics},
  language = {en},
  number = {10}
}

