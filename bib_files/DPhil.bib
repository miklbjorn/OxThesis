  doi = {10.1016/0370-2693(95)00285-S},
  abstract = {Both Cabibbo allowed and doubly forbidden transitions contribute coherently to D \textrightarrow{} KS,L + {$\pi{'}$}s decays. This leads to several intriguing and even quantitatively significant consequences, among them (i) A difference between {$\Gamma$}(D+ \textrightarrow{} KS{$\pi$}+) and {$\Gamma$}(D+ \textrightarrow{} KL{$\pi$}+) and between {$\Gamma$}(D0 \textrightarrow{} KS{$\pi$}0) and {$\Gamma$}(D0 \textrightarrow{} KL{$\pi$}0) of roughly 10\%; similarly {$\Gamma$}(D+ \textrightarrow{} [KS{$\pi$}0]K{${_\ast}\pi$}+) {$\neq$} 14{$\Gamma$}(D+ \textrightarrow{} [K-{$\pi$}+]K{${_\ast}\pi$}+), and more generally {$\Gamma$}(D \textrightarrow{} K0 + {$\pi{'}$}s) {$\neq$} 2{$\Gamma$}(D \textrightarrow{} KS + {$\pi{'}$}s). (ii) A change in the relative phase between the isospin 32 and 12 amplitudes as extracted from the observed branching ratios for D+ \textrightarrow{} KS{$\pi$}+, D0 \textrightarrow{} KS{$\pi$}0, K-{$\pi$}+. (iii) If New Physics intervenes to provide the required weak phase, then CP asymmetries of up to a few per cent can arise in D+ \textrightarrow{} KS{$\pi$}+ vs. D- \textrightarrow{} KS{$\pi-$}, D0 \textrightarrow{} KS{$\pi$}0 vs. D0 \textrightarrow{} KS{$\pi$}0, D+ \textrightarrow{} [KS{$\pi$}0]K{${_\ast}\pi$}+ vs. D- \textrightarrow{} [KS{$\pi$}0]K{${_\ast}\pi-$}, etc.; an asymmetry of the same size, but opposite in sign occurs when the KS is replaced by a KL in the final state.},
  file = {/Users/bjoern/Zotero/storage/YNPLDDG2/Bigi and Yamamoto - 1995 - Interference between Cabibbo allowed and doubly fo.pdf;/Users/bjoern/Zotero/storage/53B8B48B/037026939500285S.html},
  journal = {Physics Letters B},
  language = {en},
  number = {3}
}

@article{birdComputingLargeHadron2011a,
  title = {Computing for the {{Large Hadron Collider}}},
  author = {Bird, Ian},
  year = {2011},
  month = oct,
  volume = {61},
  pages = {99--118},
  publisher = {{Annual Reviews}},
  issn = {0163-8998},
  doi = {10.1146/annurev-nucl-102010-130059},
  abstract = {Following the first full year of Large Hadron Collider (LHC) data taking, the Worldwide LHC Computing Grid (WLCG) computing environment built to support LHC data processing and analysis has been validated. In this review, I discuss the rationale for the design of a distributed system and describe how this environment was constructed and deployed through the use of grid computing technologies. I discuss the experience with large-scale testing and operation with real accelerator data, which shows that expectations have been met and sometimes exceeded. The computing system's key achievements are that (a) the WLCG infrastructure is distributed and makes use of all the dispersed resources, (b) the experiments' computing models are also distributed and can make excellent use of the infrastructure, and (c) the computing system has enabled physics output in a very short time. Finally, I present prospects for the future evolution of the WLCG infrastructure.},
  file = {/Users/bjoern/Zotero/storage/QJLUW5XN/annurev-nucl-102010-130059.html},
  journal = {Annual Review of Nuclear and Particle Science},
  number = {1}
}

