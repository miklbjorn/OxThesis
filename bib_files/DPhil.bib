  eprint = {0801.0840},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/2HQLFABI/Bondar and Poluektov - 2008 - The use of quantum-correlated D0 decays for φ3 mea.pdf},
  journal = {The European Physical Journal C},
  language = {en},
  number = {1}
}

