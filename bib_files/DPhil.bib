@article{thebabarcollaborationEvidenceDirectCP2010,
  title = {Evidence for Direct {{CP}} Violation in the Measurement of the {{Cabibbo}}-{{Kobayashi}}-{{Maskawa}} Angle Gamma with {{B}}-+ --{$>$} {{D}}(*) {{K}}(*)-+ Decays},
  author = {The BABAR Collaboration and Sanchez, P. del Amo},
  year = {2010},
  month = sep,
  volume = {105},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.105.121801},
  abstract = {We report the measurement of the Cabibbo-Kobayashi-Maskawa CP-violating angle \$\textbackslash gamma\$ through a Dalitz plot analysis of neutral \$D\$ meson decays to \$K\_S\^0 \textbackslash pi\^+ \textbackslash pi\^-\$ and \$K\_S\^0 K\^+ K\^-\$ produced in the processes \$B\^\textbackslash mp \textbackslash to D K\^\textbackslash mp\$, \$B\^\textbackslash mp \textbackslash to D\^\{*\} K\^\textbackslash mp\$ with \$D\^* \textbackslash to D \textbackslash pi\^0,D \textbackslash gamma\$, and \$B\^\textbackslash mp \textbackslash to D K\^\{*\textbackslash mp\}\$ with \$K\^\{*\textbackslash mp\} \textbackslash to K\_S\^0 \textbackslash pi\^\textbackslash mp\$, using 468 million \$B\textbackslash bar\{B\}\$ pairs collected by the BABAR detector at the PEP-II asymmetric-energy \$e\^+e\^-\$ collider at SLAC. We measure \$\textbackslash gamma=(68 \textbackslash pm 14 \textbackslash pm 4 \textbackslash pm 3)\^\textbackslash circ\$ (\{\textbackslash rm mod \$180\^\textbackslash circ\$\}), where the first error is statistical, the second is the experimental systematic uncertainty and the third reflects the uncertainty in the description of the neutral \$D\$ decay amplitudes. This result is inconsistent with \$\textbackslash gamma = 0\$ (no direct CP violation) with a significance of 3.5 standard deviations.},
  archivePrefix = {arXiv},
  eprint = {1005.1096},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/KIHID8AA/The BABAR Collaboration and Sanchez - 2010 - Evidence for direct CP violation in the measuremen.pdf;/Users/bjoern/Zotero/storage/AAMPZ2PT/1005.html},
  journal = {Physical Review Letters},
  keywords = {High Energy Physics - Experiment},
  number = {12}
}

@article{thebabarcollaborationMeasurementCPObservables2010,
  title = {{Measurement of CP observables in $B^\pm \to D_{CP} K^\pm)$ decays and constraints on the CKM angle gamma}},
  author = {Pedro del Amo Sanchez and others},
  year = {2010},
  month = oct,
  volume = {82},
  pages = {072004},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.82.072004},
  abstract = {Using the entire sample of 467 million Y(4S) --> BBbar decays collected with the BaBar detector at the PEP-II asymmetric-energy B factory at SLAC, we perform a "GLW" analysis of B+- --> D K+- decays, using decay modes in which the neutral D meson decays to either CP-eigenstates or non-CP-eigenstates. We measure the partial decay rate charge asymmetries for CP-even and CP-odd D final states to be A_CP+ = 0.25+-0.06+-0.02 and A_CP- = -0.09+-0.07+-0.02, respectively, where the first error is the statistical and the second is the systematic uncertainty. The parameter A_CP+ is different from zero with a significance of 3.6 standard deviations, constituting evidence for direct CP violation. We also measure the ratios of the charged-averaged B partial decay rates in CP and non-CP decays, R_CP+ = 1.18+-0.09+-0.05 and R_CP- = 1.07+-0.08+-0.04. We infer frequentist confidence intervals for the angle gamma of the (db) unitarity triangle, for the strong phase difference delta_B, and for the amplitude ratio r_B, which are related to the B- --> DK- decay amplitude by r_Be^{i(delta_B-gamma)} = A(B- --> anti-D0 K^-)/A(B- --> D0 K-). Including statistical and systematic uncertainties, we obtain 0.24<r_B<0.45 (0.06<r_B<0.51) and, modulo 180 degrees, 11.3 degrees < gamma < 22.7 degrees or 80.9 degrees < gamma < 99.1 degrees or 157.3 degrees < gamma < 168.7 degrees (7.0 degrees < gamma < 173.0 degrees) at the 68% (95%) confidence level.},
  archivePrefix = {arXiv},
  collaboration = {\babar collaboration},
  eprint = {1007.0504},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/2ZVMPY8C/The BABAR Collaboration and Sanchez - 2010 - Measurement of CP observables in B+- -- D_CP K+- .pdf;/Users/bjoern/Zotero/storage/5XS6BTAK/1007.html},
  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {7}
}

@article{thebabarcollaborationMeasurementD0antiD0Mixing2010,
  title = {Measurement of {{D0}}-{{antiD0}} Mixing Parameters Using {{D0}} -{$>$} {{K0S}} Pi+ Pi- and {{D0}} -{$>$} {{KS0 K}}+ {{K}}- Decays},
  author = {The BABAR Collaboration and Sanchez, P. del Amo},
  year = {2010},
  month = aug,
