@article{maldeFirstDeterminationCP2015,
  title = {First Determination of the {{CP}} Content of {{D}}\textrightarrow{$\pi$}+{$\pi-\pi$}+{$\pi-$} and Updated Determination of the {{CP}} Contents of {{D}}\textrightarrow{$\pi$}+{$\pi-\pi$}0 and {{D}}\textrightarrow{{K}}+{{K}}-{$\pi$}0},
  author = {Malde, S. and Thomas, C. and Wilkinson, G. and Naik, P. and Prouve, C. and Rademacker, J. and Libby, J. and Nayak, M. and Gershon, T. and Briere, R. A.},
  year = {2015},
  month = jul,
  volume = {747},
  pages = {9--17},
  issn = {0370-2693},
  doi = {10.1016/j.physletb.2015.05.043},
  abstract = {Quantum-correlated {$\psi$}(3770)\textrightarrow DD- decays collected by the CLEO-c experiment are used to perform a first measurement of F+4{$\pi$}, the fractional CP-even content of the self-conjugate decay D\textrightarrow{$\pi$}+{$\pi-\pi$}+{$\pi-$}, obtaining a value of 0.737{$\pm$}0.028. An important input to the measurement comes from the use of D\textrightarrow KS0{$\pi$}+{$\pi-$} and D\textrightarrow KL0{$\pi$}+{$\pi-$} decays to tag the signal mode. This same technique is applied to the channels D\textrightarrow{$\pi$}+{$\pi-\pi$}0 and D\textrightarrow K+K-{$\pi$}0, yielding F+{$\pi\pi\pi$}0=1.014{$\pm$}0.045{$\pm$}0.022 and F+KK{$\pi$}0=0.734{$\pm$}0.106{$\pm$}0.054, where the first uncertainty is statistical and the second systematic. These measurements are consistent with those of an earlier analysis, based on CP-eigenstate tags, and can be combined to give values of F+{$\pi\pi\pi$}0=0.973{$\pm$}0.017 and F+KK{$\pi$}0=0.732{$\pm$}0.055. The results will enable the three modes to be included in a model-independent manner in measurements of the unitarity triangle angle {$\gamma$} using B{$\mp\rightarrow$}DK{$\mp$} decays, and in time-dependent studies of CP violation and mixing in the D0D-0 system.},
  file = {/Users/bjoern/Zotero/storage/NUBAX46V/Malde et al. - 2015 - First determination of the CP content of D→π+π−π+π.pdf;/Users/bjoern/Zotero/storage/RZ32I87A/S0370269315003809.html},
  journal = {Physics Letters B},
  keywords = {Charm decay,Quantum correlations,violation},
  language = {en}
}

@article{muramatsuDalitzAnalysisPi2002,
  title = {Dalitz {{Analysis}} of {{D}}\^0 --{$>$} {{K}}\_{{S}}\^0 Pi+ Pi-},
  author = {Muramatsu, H. and Collaboration, CLEO},
  year = {2002},
  month = dec,
  volume = {89},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.89.251802},
  abstract = {In e\^+e\^- collisions using the CLEO detector we have studied the decay of the D\^0 to the final state K\_S\^0 pi\^+ pi\^- with the initial flavor of the D\^0 tagged in charged D\^* decay. We use the Dalitz technique to measure the resonant substructure in this final state and clearly observe ten different contributions by fitting for their amplitudes and relative phases. We observe a K\^\{* +\} pi\^- component which arises from doubly Cabibbo suppressed decays or D\^0 - \{D\^0\}-bar mixing.},
  archivePrefix = {arXiv},
  eprint = {hep-ex/0207067},
  eprinttype = {arxiv},
  file = {/Users/bjoern/Zotero/storage/V722R98L/Muramatsu and Collaboration - 2002 - Dalitz Analysis of D^0 -- K_S^0 pi+ pi-.pdf;/Users/bjoern/Zotero/storage/D2SXH8ZJ/0207067.html},
  journal = {Physical Review Letters},
  keywords = {High Energy Physics - Experiment},
  number = {25}
}

@article{nayakFirstDeterminationCP2015,
