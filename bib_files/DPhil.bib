  journal = {Physical Review D},
  keywords = {High Energy Physics - Experiment},
  number = {3}
}

@article{cdfcollaborationMeasurementsBranchingFraction2011,
  title = {{Measurements of branching fraction ratios and CP-asymmetries in suppressed $B^- \to D(\to K^+ \pi^-)K^-$ and $B^- \to D(-> K^+ \pi^-)\pi^-$ decays}},
