  author = {Ahn, Y. H. and Cheng, Hai-Yang and Oh, Sechul},
  year = {2011},
  month = sep,
  volume = {703},
  pages = {571--575},
  issn = {0370-2693},
  doi = {10.1016/j.physletb.2011.08.047},
  abstract = {The precise determination of the Cabibbo\textendash Kobayashi\textendash Maskawa (CKM) matrix elements is extremely important towards the understanding of CP violation. We explicitly study seeming discrepancies between the CKM matrix elements at the higher order of the expansion parameter {$\lambda$} in different Wolfenstein parametrizations derived from different exact parametrizations. A systematic way of resolving the seeming discrepancies is proposed. We find that most of the discrepancies can be naturally resolved by a proper redefinition of the numerically small (of order {$\lambda$}) parameters. Our approach is further applied to the cases for the Wolfenstein-like parametrizations, such as the Qin\textendash Ma parametrization.},
  file = {/Users/bjoern/Zotero/storage/Q5975IH8/Ahn et al. - 2011 - Wolfenstein parametrization at higher order Seemi.pdf;/Users/bjoern/Zotero/storage/8VHW7I8I/S0370269311009956.html},
  journal = {Physics Letters B},
  language = {en},
  number = {5}
}

@article{atwoodEnhancedCPViolation1997,
  title = {Enhanced {{CP Violation}} with \$\textbackslash mathit\{\vphantom\}{{B}}\vphantom\{\}\textbackslash ensuremath\{\textbackslash rightarrow\}\{\textbackslash mathrm\{\vphantom{\}\}}{{KD}}\vphantom\{\}\vphantom\{\}\^\{0\}(\{\textbackslash overline\{\vphantom{\}\}}{{D}}\vphantom\{\}\vphantom\{\}\^\{0\})\$ {{Modes}} and {{Extraction}} of the {{Cabibbo}}-{{Kobayashi}}-{{Maskawa Angle}} \$\textbackslash ensuremath\{\textbackslash gamma\}\$},
