@article{drozd_comparing_2015,
	title = {Comparing {EFT} and {Exact} {One}-{Loop} {Analyses} of {Non}-{Degenerate} {Stops}},
	volume = {1506},
	doi = {10.1007/JHEP06(2015)028},
	abstract = {We develop a universal approach to the one-loop effective field theory (EFT) using the Covariant Derivative Expansion (CDE) method. We generalise previous results to include broader classes of UV models, showing how expressions previously obtained assuming degenerate heavy-particle masses can be extended to non-degenerate cases. We apply our method to the general MSSM with non-degenerate stop squarks, illustrating our approach with calculations of the coefficients of dimension-6 operators contributing to the \$hgg\$ and \$h{\textbackslash}gamma{\textbackslash}gamma\$ couplings, and comparing with exact calculations of one-loop Feynman diagrams. We then use present and projected future sensitivities to these operator coefficients to obtain present and possible future indirect constraints on stop masses. The current sensitivity is already comparable to that of direct LHC searches, and future FCC-ee measurements could be sensitive to stop masses above a TeV. The universality of our one-loop EFT approach facilitates extending these constraints to a broader class of UV models.},
	journal = {JHEP},
	author = {Drozd, Aleksandra and Ellis, John and Quevillon, Jérémie and You, Tevong},
	month = jun,
	year = {2015},
	keywords = {effective field theory, derivative: covariance, expansion: derivative, expansion: higher-order, Higgs particle: coupling, higher-order: 1, minimal supersymmetric standard model, new physics, operator: dimension: 6, Phenomenology of Field Theories in Higher Dimensions, stop: mass, supersymmetry, Supersymmetry Phenomenology, unitarity},
	pages = {028},
	file = {INSPIRE Snapshot:/Users/bjoern/Zotero/storage/V4TGZIP9/1358640.html:text/html}
}

