@article{balazs_soft_1997,
	title = {Soft gluon effects on lepton pairs at hadron colliders},
	volume = {56},
	url = {http://link.aps.org/doi/10.1103/PhysRevD.56.5558},
	doi = {10.1103/PhysRevD.56.5558},
	abstract = {We consider the Collins-Soper-Sterman resummation formalism, which describes the vector boson transverse momentum (QT) distribution at hadron colliders, and extend it for the distributions of the decay leptons by correctly including the effects of the polarization and the width of the vector boson. Numerous aspects of the formalism are reviewed at the O(α2S) level, including the matching of the QT distribution and the value of the total cross section. Detailed comparisons of several O(αS) fixed order (NLO) and resummed lepton distributions are presented. The total rates and the distributions of the lepton charge asymmetry predicted by the resummed and the NLO calculations are shown to differ when kinematic cuts are applied. We also show how to test the rich dynamics of the QCD multiple soft gluon radiation, among others by measuring the ratio RCSS≡σ(QT{\textgreater}QminT)/σtotal.},
	number = {9},
	urldate = {2016-05-29},
	journal = {Phys. Rev. D},
	author = {Balázs, C. and Yuan, C.-P.},
	month = nov,
	year = {1997},
	pages = {5558--5583},
	file = {APS Snapshot:/Users/bjoern/Zotero/storage/IMEGVQSN/PhysRevD.56.html:text/html;Full Text PDF:/Users/bjoern/Zotero/storage/KAWF9S5J/Balázs and Yuan - 1997 - Soft gluon effects on lepton pairs at hadron colli.pdf:application/pdf}
}

@article{cdf_collaboration_direct_2008,
	title = {A direct {Measurement} of the {W} {Boson} {Width} in ppbar {Collisions} at sqrt(s) = 1.96 {TeV}},
	volume = {100},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/0710.4112},
	doi = {10.1103/PhysRevLett.100.071801},
	abstract = {A direct measurement of the total decay width of the W boson G\_W is presented using 350 pb-1 of data from ppbar collisions at sqrt(s) = 1.96 TeV collected with the CDF II detector at the Fermilab Tevatron. The width is determined by normalizing predicted signal and background distributions to 230185 W candidates decaying to enu and munu in the transverse-mass region 50 {\textless} M\_T {\textless} 90 GeV and then fitting the predicted shape to 6055 events in the high-M\_T region, 90 {\textless} M\_T {\textless} 200 GeV. The result is G\_W = 2032 +/- 45 (stat.) +/- 57 (syst.) MeV, consistent with the standard model expectation.},
	number = {7},
	urldate = {2016-05-29},
	journal = {Physical Review Letters},
	author = {CDF Collaboration and Aaltonen, T.},
	month = feb,
	year = {2008},
	note = {arXiv: 0710.4112},
	keywords = {High Energy Physics - Experiment},
	file = {arXiv\:0710.4112 PDF:/Users/bjoern/Zotero/storage/PVGRK4W4/CDF Collaboration and Aaltonen - 2008 - A direct Measurement of the W Boson Width in ppbar.pdf:application/pdf;arXiv.org Snapshot:/Users/bjoern/Zotero/storage/E4QW5HZC/0710.html:text/html}
}

@article{d0_collaboration_direct_2009,
	title = {Direct measurement of the {W} boson width},
	volume = {103},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/0909.4814},
	doi = {10.1103/PhysRevLett.103.231802},
	abstract = {We present a direct measurement of the width of the W boson using the shape of the transverse mass distribution of W-{\textgreater}enu candidates selected in 1 fb-1 of data collected with the D0 detector at the Fermilab Tevatron collider in ppbar collisions at sqrt\{s\}=1.96 TeV. We use the same methods and data sample that were used for our recently published W boson mass measurement, except for the modeling of the recoil, which is done with a new method based on a recoil library. Our result, 2.028 +- 0.072 GeV, is in agreement with the predictions of the standard model.},
