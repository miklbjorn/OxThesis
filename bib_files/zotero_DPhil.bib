@article{cdf_collaboration_combination_2013,
	title = {Combination of {CDF} and {D0} {W}-{Boson} {Mass} {Measurements}},
	volume = {88},
	issn = {1550-7998, 1550-2368},
	url = {http://arxiv.org/abs/1307.7627},
	doi = {10.1103/PhysRevD.88.052018},
	abstract = {We summarize and combine direct measurements of the mass of the \$W\$ boson in \${\textbackslash}sqrt\{s\} = 1.96 {\textbackslash}text\{TeV\}\$ proton-antiproton collision data collected by CDF and D0 experiments at the Fermilab Tevatron Collider. Earlier measurements from CDF and D0 are combined with the two latest, more precise measurements: a CDF measurement in the electron and muon channels using data corresponding to \$2.2 {\textbackslash}mathrm\{fb\}{\textasciicircum}\{-1\}\$ of integrated luminosity, and a D0 measurement in the electron channel using data corresponding to \$4.3 {\textbackslash}mathrm\{fb\}{\textasciicircum}\{-1\}\$ of integrated luminosity. The resulting Tevatron average for the mass of the \$W\$ boson is \${\textbackslash}MW = 80{\textbackslash},387 {\textbackslash}pm 16 {\textbackslash}text\{MeV\}\$. Including measurements obtained in electron-positron collisions at LEP yields the most precise value of \${\textbackslash}MW = 80{\textbackslash},385 {\textbackslash}pm 15 {\textbackslash}text\{MeV\}\$.},
	number = {5},
