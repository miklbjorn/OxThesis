@article{bouchendira_new_2011,
	title = {New determination of the fine structure constant and test of the quantum electrodynamics},
	volume = {106},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/1012.3627},
	doi = {10.1103/PhysRevLett.106.080801},
	abstract = {We report a new measurement of the ratio \$h/m\_\{{\textbackslash}mathrm\{Rb\}\}\$ between the Planck constant and the mass of \${\textasciicircum}\{87\}{\textbackslash}mathrm\{Rb\}\$ atom. A new value of the fine structure constant is deduced, \${\textbackslash}alpha{\textasciicircum}\{-1\}=137.035{\textbackslash},999{\textbackslash},037{\textbackslash},(91)\$ with a relative uncertainty of \$6.6{\textbackslash}times 10{\textasciicircum}\{-10\}\$. Using this determination, we obtain a theoretical value of the electron anomaly \$a\_{\textbackslash}mathrm\{e\}=0.001{\textasciitilde}159{\textasciitilde}652{\textasciitilde}181{\textasciitilde}13(84)\$ which is in agreement with the experimental measurement of Gabrielse (\$a\_{\textbackslash}mathrm\{e\}=0.001{\textasciitilde}159{\textasciitilde}652{\textasciitilde}180{\textasciitilde}73(28)\$). The comparison of these values provides the most stringent test of the QED. Moreover, the precision is large enough to verify for the first time the muonic and hadronic contributions to this anomaly.},
	number = {8},
	urldate = {2016-05-28},
	journal = {Physical Review Letters},
	author = {Bouchendira, Rym and Cladé, Pierre and Guellati-Khélifa, Saïda and Nez, François and Biraben, François},
