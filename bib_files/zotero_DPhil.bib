	note = {arXiv: hep-ex/0509008},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology},
	pages = {257--454},
	file = {arXiv\:hep-ex/0509008 PDF:/Users/bjoern/Zotero/storage/3BXXPBTT/The ALEPH Collaboration et al. - 2006 - Precision Electroweak Measurements on the Z Resona.pdf:application/pdf;arXiv.org Snapshot:/Users/bjoern/Zotero/storage/D7UHWNTR/0509008.html:text/html}
