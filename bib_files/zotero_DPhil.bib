	volume = {304},
	issn = {0550-3213},
	url = {http://www.sciencedirect.com/science/article/pii/0550321388906384},
	doi = {10.1016/0550-3213(88)90638-4},
	abstract = {The cross section for e+e− → W+W− with arbitrary polarizations of the leptons and bosons is calculated in the standard electroweak model including the complete one-loop virtual and soft-photon bremsstrahlung corrections. The relevant analytic results are listed and the influence of the radiative corrections on the unpolarized cross section is discussed. We find moderate corrections at LEP II energies. The corrections increase continuously with energy up to − 50\% of the Born cross section at 1 TeV.},
	urldate = {2015-12-07},
	journal = {Nuclear Physics B},
	author = {Böhm, M. and Denner, A. and Sack, T. and Beenakker, W. and Berends, F. and Kuijf, H.},
