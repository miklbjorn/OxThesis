	file = {arXiv\:1008.4884 PDF:/Users/bjoern/Zotero/storage/2FBF6CVF/Grzadkowski m.fl. - 2010 - Dimension-Six Terms in the Standard Model Lagrangi.pdf:application/pdf;arXiv.org Snapshot:/Users/bjoern/Zotero/storage/RRGESSKZ/1008.html:text/html}
}

