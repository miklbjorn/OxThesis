@article{wells_effective_2015,
	title = {Effective theories of universal theories},
	url = {http://arxiv.org/abs/1510.08462},
	abstract = {It is well-known but sometimes overlooked that constraints on the oblique parameters (most notably \$S\$ and \$T\$ parameters) are only applicable to a special class of new physics scenarios known as universal theories. In the effective field theory (EFT) framework, the oblique parameters should not be associated with Wilson coefficients in a particular operator basis, unless restrictions have been imposed on the EFT so that it describes universal theories. We work out these restrictions, and present a detailed EFT analysis of universal theories. We find that at the dimension-6 level, universal theories are completely characterized by 16 parameters. They are conveniently chosen to be: 5 oblique parameters that agree with the commonly-adopted ones, 4 anomalous triple-gauge couplings, 3 rescaling factors for the \$h{\textasciicircum}3\$, \$hff\$, \$hVV\$ vertices, 3 parameters for \$hVV\$ vertices absent in the Standard Model, and 1 four-fermion coupling of order \$y\_f{\textasciicircum}2\$. All these parameters are defined in an unambiguous and basis-independent way, allowing for consistent constraints on the universal theories parameter space from precision electroweak and Higgs data.},
	urldate = {2015-11-03},
	journal = {arXiv:1510.08462 [hep-ph]},
	author = {Wells, James D. and Zhang, Zhengkang},
	month = oct,
	year = {2015},
	note = {arXiv: 1510.08462},
	keywords = {High Energy Physics - Phenomenology},
	file = {arXiv\:1510.08462 PDF:/Users/bjoern/Zotero/storage/S3USV63M/Wells and Zhang - 2015 - Effective theories of universal theories.pdf:application/pdf;arXiv.org Snapshot:/Users/bjoern/Zotero/storage/CV397M9W/1510.html:text/html}
}

