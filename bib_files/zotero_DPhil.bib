@article{giri_determining_2003-1,
	title = {Determining \${\textbackslash}gamma\$ using \${B}{\textasciicircum}{\textbackslash}pm {\textbackslash}to {D} {K}{\textasciicircum}{\textbackslash}pm\$ with multibody {D} decays},
	volume = {68},
	issn = {0556-2821, 1089-4918},
	url = {http://arxiv.org/abs/hep-ph/0303187},
	doi = {10.1103/PhysRevD.68.054018},
	abstract = {We propose a method for determining \${\textbackslash}gamma\$ using \$B{\textasciicircum}{\textbackslash}pm{\textbackslash}to D K{\textasciicircum}{\textbackslash}pm\$ decays followed by a multibody \$D\$ decay, such as \$D {\textbackslash}to K\_S {\textbackslash}pi{\textasciicircum}-{\textbackslash}pi{\textasciicircum}+\$, \$D {\textbackslash}to K\_S K{\textasciicircum}-K{\textasciicircum}+\$ and \$D {\textbackslash}to K\_S {\textbackslash}pi{\textasciicircum}-{\textbackslash}pi{\textasciicircum}+{\textbackslash}pi{\textasciicircum}0\$. The main advantages of the method is that it uses only Cabibbo allowed \$D\$ decays, and that large strong phases are expected due to the presence of resonances. Since no knowledge about the resonance structure is needed, \${\textbackslash}gamma\$ can be extracted without any hadronic uncertainty.},
	number = {5},
	urldate = {2017-12-12},
	journal = {Physical Review D},
	author = {Giri, Anjan and Grossman, Yuval and Soffer, Abner and Zupan, Jure},
	month = sep,
