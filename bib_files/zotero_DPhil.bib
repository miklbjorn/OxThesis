}

@article{lhcb_collaboration_measurement_2014,
	title = {Measurement of the {CKM} angle {\textbackslash}gamma using {B}{\textasciicircum}{\textbackslash}pm --{\textgreater} {D} {K}{\textasciicircum}{\textbackslash}pm with {D}--{\textgreater}{K}\_S{\textbackslash}pi{\textasciicircum}+{\textbackslash}pi{\textasciicircum}-, {K}\_SK{\textasciicircum}+{K}{\textasciicircum}- decays},
	volume = {2014},
	issn = {1029-8479},
	url = {http://arxiv.org/abs/1408.2748},
	doi = {10.1007/JHEP10(2014)097},
	abstract = {A binned Dalitz plot analysis of \$B{\textasciicircum}{\textbackslash}pm {\textbackslash}to D K{\textasciicircum}{\textbackslash}pm\$ decays, with \$D {\textbackslash}to K\_S {\textbackslash}pi{\textasciicircum}+{\textbackslash}pi{\textasciicircum}-\$ and \$D {\textbackslash}to K\_S K{\textasciicircum}+ K{\textasciicircum}-\$, is performed to measure the {\textbackslash}CP-violating observables \$x\_\{{\textbackslash}pm\}\$ and \$y\_\{{\textbackslash}pm\}\$, which are sensitive to the Cabibbo-Kobayashi-Maskawa angle \${\textbackslash}gamma\$. The analysis exploits a sample of proton-proton collision data corresponding to 3.0{\textbackslash}invfb collected by the LHCb experiment. Measurements from CLEO-c of the variation of the strong-interaction phase of the \$D\$ decay over the Dalitz plot are used as inputs. The values of the parameters are found to be \$x\_+ = (-7.7 {\textbackslash}pm 2.4 {\textbackslash}pm 1.0 {\textbackslash}pm 0.4){\textbackslash}times 10{\textasciicircum}\{-2\}\$, \$x\_- = (2.5 {\textbackslash}pm 2.5 {\textbackslash}pm 1.0 {\textbackslash}pm 0.5) {\textbackslash}times 10{\textasciicircum}\{-2\}\$, \$y\_+ = (-2.2 {\textbackslash}pm 2.5 {\textbackslash}pm 0.4 {\textbackslash}pm 1.0){\textbackslash}times 10{\textasciicircum}\{-2\}\$, and \$y\_- = (7.5 {\textbackslash}pm 2.9 {\textbackslash}pm 0.5 {\textbackslash}pm 1.4) {\textbackslash}times 10{\textasciicircum}\{-2\}\$. The first, second, and third uncertainties are the statistical, the experimental systematic, and that associated with the precision of the strong-phase parameters. These are the most precise measurements of these observables and correspond to \${\textbackslash}gamma = (62{\textasciicircum}\{{\textbackslash},+15\}\_\{{\textbackslash},-14\}){\textasciicircum}{\textbackslash}circ\$, with a second solution at \${\textbackslash}gamma {\textbackslash}to {\textbackslash}gamma + 180{\textasciicircum}{\textbackslash}circ\$, and \$r\_B = 0.080{\textasciicircum}\{+ 0.019\}\_\{-0.021\}\$, where \$r\_B\$ is the ratio between the suppressed and favoured \$B\$ decay amplitudes.},
